<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>finalWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>HelloForm</servlet-name>
  	<servlet-class>model.HttpServletSample</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloForm</servlet-name>
  	<url-pattern>/finalWeb/HelloForm</url-pattern>
  </servlet-mapping>
  <error-page>
  	<error-code>404</error-code>
  	<location>/notFind.jsp</location>
  </error-page>
  <filter>
  	<filter-name>LogFilter</filter-name>
  	<filter-class>model.LogFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>LogFilter</filter-name>
  	<url-pattern>/register.jsp</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>HelloForm2</servlet-name>
  	<servlet-class>model.HttpServletSample2</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloForm2</servlet-name>
  	<url-pattern>/finalWeb/HelloForm2</url-pattern>
  </servlet-mapping>
  <filter-mapping>
  	<filter-name>LogFilter</filter-name>
  	<url-pattern>/update.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>LogFilter</filter-name>
  	<url-pattern>/delete.jsp</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>HelloForm3</servlet-name>
  	<servlet-class>model.HttpServletSample3</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloForm3</servlet-name>
  	<url-pattern>/finalWeb/HelloForm3</url-pattern>
  </servlet-mapping>
</web-app>